{
	"name": "@astray/astray",
	"version": "0.0.0",
	"private": true,
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"scripts": {
		"build": "turbo run build",
		"dev": "turbo run dev --parallel",
		"lint": "turbo run lint",
		"format": "prettier --write \"**/*.{ts,tsx,md}\""
	},
	"devDependencies": {
		"eslint-config-custom": "workspace:^",
		"prettier": "^2.8.3",
		"turbo": "^1.7.2",
		"typescript": "^4.9.5"
	},
	"engines": {
		"npm": ">=7.0.0",
		"node": ">=18.0.0"
	},
	"packageManager": "yarn@3.4.1"
}
